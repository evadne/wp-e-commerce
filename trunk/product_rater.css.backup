.starimage{
    /* for Internet Explorer, dont you love how unsupported CSS elements are ignored by other browsers */
    filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='images/24bit-star.png',sizingMethod='scale');
    }   
           
a.star1{
    display:block;
    width: 18px;
    text-align: right;
    z-index: 5;
    position: relative;
    top: 0px;
    left: 0px;
    }
    
a.star1:hover{
    /*background: #46F;*/
    background: url(images/blue-star.gif);
    }
    
